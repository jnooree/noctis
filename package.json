{
  "name": "noctis",
  "displayName": "Noctis",
  "description": "Noctis is a collection of light & dark themes with a well balanced blend of warm and cold colors",
  "version": "0.0.0",
  "publisher": "jnooree",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.43.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "galleryBanner": {
    "color": "#052529",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnooree/noctis"
  },
  "scripts": {
    "build": "node src/build.mjs"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "noctis",
    "blue",
    "azure",
    "semantic",
    "italic",
    "bold"
