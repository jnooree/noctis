{
  "name": "semantic-noctis",
  "displayName": "Semantic Noctis",
  "description": "Semantic Noctis is a modified version of Noctis Lux and Noctis Minimus, with semantic highlighting support.",
  "version": "1.3.1",
  "publisher": "jnooree",
  "icon": "images/logo.png",
  "engines": {
    "vscode": "^1.43.0"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "galleryBanner": {
    "color": "#052529",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jnooree/noctis"
  },
  "scripts": {
    "build": "node src/build.mjs",
    "deploy": "vsce publish"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "keywords": [
    "noctis",
    "blue",
    "azure",
    "semantic",
    "italic",
    "bold"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Semantic Noctis Lux",
        "uiTheme": "vs",
        "path": "./themes/lux.json"
      },
      {
        "label": "Semantic Noctis Minimus",
        "uiTheme": "vs-dark",
        "path": "./themes/minimus.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/vsce": "^2.22.0"
  }
}
